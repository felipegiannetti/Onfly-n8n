{
  "name": "n8n-nodes-random",
  "version": "1.0.0",
  "description": "Processo Recruta Onfly - Desafio custom node n8n",
  "author": "Felipe Giannetti Fontenelle",
  "license": "MIT",
  "type": "commonjs",
  "main": "dist/Random.node.js",
  "scripts": {
    "build": "tsc -p tsconfig.json && npm run copy-resources",
    "copy-resources": "if not exist dist\\resources mkdir dist\\resources && copy resources\\* dist\\resources\\",
    "dev": "tsc -w -p tsconfig.json",
    "clean": "rimraf dist",
    "test": "npm run build && node --test test/*.js"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.5.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "n8n-workflow": "^1.0.0"
  },
  "n8n": {
    "nodes": [
      "dist/Random.node.js"
    ]
  }
}
